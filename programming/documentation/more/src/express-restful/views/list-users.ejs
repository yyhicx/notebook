<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Express Example</title>
    <link
      href="https://cdn.staticfile.org/twitter-bootstrap/5.1.2/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <script src="https://cdn.staticfile.org/bootstrap/5.1.2/js/bootstrap.bundle.min.js"></script>
  </head>
  <body>
    <div class="container mt-3">
      <table
        class="table m-auto table-responsive table-striped table-hover"
        style="max-width: 600px"
      >
        <thead>
          <tr>
            <th>Name</th>
            <th>Password</th>
            <th>Profession</th>
          </tr>
        </thead>
        <% for(let i = 0; i < data.length; i++) {%>
        <tbody>
          <tr>
            <td><%= data[i].name %></td>
            <td><%= data[i].password %></td>
            <td><%= data[i].profession %></td>
          </tr>
        </tbody>
        <%} %>
      </table>
    </div>
  </body>
</html>
